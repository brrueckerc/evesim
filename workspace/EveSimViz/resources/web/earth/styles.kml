<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.2">
<Document>
    <name>earth.kml</name>
    <StyleMap id="style_PC">
        <Pair>
            <key>soapod_node</key>
            <styleUrl>#soapod_node</styleUrl>
        </Pair>
        <Pair>
            <key>real_node</key>
            <styleUrl>#real_node</styleUrl>
        </Pair>
    </StyleMap>
    
    <Style id="EveMonitoringNode_OK">
        <IconStyle>
            <color>ffffffff</color>
            <scale>1</scale>
            <Icon>
                <href>${static_context}/earth/pic/Circle_Green.png</href>
            </Icon>
            <hotSpot x="20" y="2" xunits="pixels" yunits="pixels"/>
        </IconStyle>
        <BalloonStyle> 
        <text><![CDATA[<table width="400px"><tr><td> $[description] </td></tr></table> ]]></text> 
        </BalloonStyle> 
		 <LabelStyle>
            <color>ffffffff</color>
        </LabelStyle>
    </Style>
    
